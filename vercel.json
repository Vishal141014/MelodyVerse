{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_YOUTUBE_API_KEY": "your-youtube-api-key-here",
    "NEXT_PUBLIC_SOCKET_URL": "https://melody-verse-socket-server.onrender.com",
    "NEXT_PUBLIC_APP_URL": "https://melody-verse-rho.vercel.app"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ]
} 